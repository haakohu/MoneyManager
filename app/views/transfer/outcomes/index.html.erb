<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Account</th>
      <th>Amount</th>
      <th>Description</th>
      <th>Payee</th>
      <th>Category</th>
      <th>processed</th>
      <th>Transfer date </th>
      <th colspan="3">
    </tr>
  </thead>
  <tbody>
    <%@outcomes.each do |t| %>
    <tr>
        <td><%=t.id%></td>
        <td><%=t.account.name%></td>
        <td><%=t.amount%></td>
        <td><%=t.description%></td>
        <td><%=t.payee%></td>
        <td><%=t.category.name%></td>
        <td><%=t.processed%></td>
        <td><%=t.transfer_date%></td>
        <td><%=link_to 'Show',transfer_outcome_path(t)%></td>
        <td><%=link_to 'Edit',edit_transfer_outcome_path(t)%></td>
        <td><%= link_to 'Destroy', [:transfer, t], method: :delete, data: { confirm: 'Are you sure?' }%></td>

    </tr>

    <%end%>
  </tbody>

</table>
<%=link_to 'New outcome', new_transfer_outcome_path%>
